{
  "manifest_version": 3,
  "name": "Safe your Time",
  "version": "1.0",
  "description": "Web blocking extension used in conjunction with Safe your Time software.",
  "permissions": ["alarms", "declarativeNetRequest", "declarativeNetRequestWithHostAccess"],
  "host_permissions": ["<all_urls>"],

  
  "background": {
    "service_worker": "worker.js"
  }
}