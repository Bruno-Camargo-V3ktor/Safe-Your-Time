{
  "manifest_version": 3,
  "name": "Safe your Time",
  "version": "1.0",
  "description": "---",
  "permissions": [],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["pkg/syt_plugin_web_bg.wasm", "pkg/*"],
      "matches": ["<all_urls>"]
    }
  ]
}